<header><%= link_to 'Go to Shelters', '/shelters' %> <%= link_to 'Go to Pets', '/pets' %></header>

<%= form_tag('/users') do %>
  <p><%= label_tag :name %></p>
  <%= text_field_tag :user_name %>
  <p><%= label_tag :address %></p>
  <%= text_field_tag :user_address %>
  <p><%= label_tag :city %></p>
  <%= text_field_tag :user_city %>
  <p><%= label_tag :state %></p>
  <%= text_field_tag :user_state %>
  <p><%= label_tag :zip %></p>
  <%= text_field_tag :user_zip %>

  <div><%= submit_tag 'Create User' %></div>
<% end %>
